{
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"extends": ["standard-with-typescript", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"@typescript-eslint/strict-boolean-expressions": "off",
		"@typescript-eslint/prefer-nullish-coalescing": "off",
		"promise/param-names": "off"
	}
}
